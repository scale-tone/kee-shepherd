{
    "name": "key-shepherd-vscode",
    "displayName": "key-shepherd-vscode",
    "description": "Herds secrets, passwords, keys etc. on your devbox",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Other"
    ],
    "homepage": "https://github.com/scale-tone/key-shepherd",
    "repository": {
        "type": "git",
        "url": "https://github.com/scale-tone/key-shepherd"
    },
    "bugs": {
        "url": "https://github.com/scale-tone/key-shepherd/issues"
    },
    "icon": "logo.png",
    "keywords": [
        "secrets",
        "Azure"
    ],
    "publisher": "key-shepherd",
    "license": "MIT",
    "activationEvents": [
        "*"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "key-shepherd-tree-view",
                    "name": "Secrets"
                }
            ]
        },

        "commands": [
            {
                "command": "key-shepherd-vscode.changeStorageType",
                "title": "KeyShepherd: Switch to another Metadata Storage"
            },
            {
                "command": "key-shepherd-vscode.stashAllWorkspaceSecrets",
                "title": "KeyShepherd: Stash All Secrets in this Project"
            },
            {
                "command": "key-shepherd-vscode.unstashAllWorkspaceSecrets",
                "title": "KeyShepherd: Unstash All Secrets in this Project"
            },
            {
                "command": "key-shepherd-vscode.maskSecrets",
                "title": "KeyShepherd: Mask Secrets in Current File"
            },
            {
                "command": "key-shepherd-vscode.unmaskSecrets",
                "title": "KeyShepherd: Unmask Secrets in Current File"
            },
            {
                "command": "key-shepherd-vscode.stashSecrets",
                "title": "KeyShepherd: Stash Secrets in Current File"
            },
            {
                "command": "key-shepherd-vscode.unstashSecrets",
                "title": "KeyShepherd: Unstash Secrets in Current File"
            },
            {
                "command": "key-shepherd-vscode.editor-context.superviseSecret",
                "title": "Supervise this Secret..."
            },
            {
                "command": "key-shepherd-vscode.editor-context.controlSecret",
                "title": "Manage this Secret..."
            },
            {
                "command": "key-shepherd-vscode.editor-context.insertSupervisedSecret",
                "title": "Insert a Supervised Secret Here..."
            },
            {
                "command": "key-shepherd-vscode.editor-context.insertManagedSecret",
                "title": "Insert a Managed Secret Here..."
            },
            {
                "command": "key-shepherd-vscode.editor-context.maskSecrets",
                "title": "Mask Secrets"
            },
            {
                "command": "key-shepherd-vscode.editor-context.unmaskSecrets",
                "title": "Unmask Secrets"
            },
            {
                "command": "key-shepherd-vscode.editor-context.stashSecrets",
                "title": "Stash Secrets"
            },
            {
                "command": "key-shepherd-vscode.editor-context.unstashSecrets",
                "title": "Unstash Secrets"
            },
            {
                "command": "key-shepherd-vscode.view-context.refresh",
                "title": "Refresh",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "key-shepherd-vscode.view-context.gotoSecret",
                "title": "Open"
            },
            {
                "command": "key-shepherd-vscode.view-context.forgetSecret",
                "title": "Forget this Secret"
            },
            {
                "command": "key-shepherd-vscode.view-context.forgetSecrets",
                "title": "Forget Secrets in this File"
            },
            {
                "command": "key-shepherd-vscode.view-context.stashSecrets",
                "title": "Stash Secrets in this Folder"
            },
            {
                "command": "key-shepherd-vscode.view-context.unstashSecrets",
                "title": "Unstash Secrets in this Folder"
            }
        ],
        "commandPalette": [
            {
                "command": "key-shepherd-vscode.view-context.forgetSecret",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.view-context.forgetSecrets",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.view-context.gotoSecret",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.superviseSecret",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.controlSecret",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.insertSupervisedSecret",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.insertManagedSecret",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.maskSecrets",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.unmaskSecrets",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.stashSecrets",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.editor-context.unstashSecrets",
                "when": "never"
            },
            {
                "command": "key-shepherd-vscode.view-context.refresh",
                "when": "never"
            }
        ],
        "submenus": [
            {
                "id": "key-shepherd-vscode.editor-context-submenu",
                "label": "KeyShepherd"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "submenu": "key-shepherd-vscode.editor-context-submenu"
                }
            ],
            "key-shepherd-vscode.editor-context-submenu": [
                {
                    "command": "key-shepherd-vscode.editor-context.superviseSecret",
                    "when": "editorHasSelection",
                    "group": "key-shepherd-vscode.editor-context-group1@1"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.controlSecret",
                    "when": "editorHasSelection",
                    "group": "key-shepherd-vscode.editor-context-group1@2"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.insertSupervisedSecret",
                    "group": "key-shepherd-vscode.editor-context-group1@3"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.insertManagedSecret",
                    "group": "key-shepherd-vscode.editor-context-group1@4"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.stashSecrets",
                    "group": "key-shepherd-vscode.editor-context-group2@1"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.unstashSecrets",
                    "group": "key-shepherd-vscode.editor-context-group2@2"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.maskSecrets",
                    "group": "key-shepherd-vscode.editor-context-group3@1"
                },
                {
                    "command": "key-shepherd-vscode.editor-context.unmaskSecrets",
                    "group": "key-shepherd-vscode.editor-context-group3@2"
                }
            ],

            "view/title": [
                {
                    "command": "key-shepherd-vscode.view-context.refresh",
                    "when": "view == key-shepherd-tree-view",
                    "group": "navigation"
                }
            ],

            "view/item/context": [
                {
                    "command": "key-shepherd-vscode.view-context.forgetSecret",
                    "when": "view == key-shepherd-tree-view && viewItem == tree-secret-local"
                },
                {
                    "command": "key-shepherd-vscode.view-context.forgetSecrets",
                    "when": "view == key-shepherd-tree-view && viewItem == tree-file-local"
                },
                {
                    "command": "key-shepherd-vscode.view-context.stashSecrets",
                    "when": "view == key-shepherd-tree-view && viewItem == tree-folder-local"
                },
                {
                    "command": "key-shepherd-vscode.view-context.unstashSecrets",
                    "when": "view == key-shepherd-tree-view && viewItem == tree-folder-local"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.62.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "ts-loader": "^9.2.5",
        "webpack": "^5.52.1",
        "webpack-cli": "^4.8.0",
        "@vscode/test-electron": "^1.6.2"
    },
    "extensionDependencies": [
        "ms-vscode.azure-account"
    ],
    "dependencies": {
        "@azure/arm-resourcegraph": "^4.2.1",
        "@azure/arm-storage": "^16.1.2",
        "@azure/data-tables": "^13.0.0",
        "@azure/keyvault-secrets": "^4.3.0",
        "@azure/ms-rest-azure-env": "^2.0.0",
        "@azure/ms-rest-nodeauth": "^3.1.0",
        "axios": "^0.24.0"
    }
}
