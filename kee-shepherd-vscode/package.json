    },
    "bugs": {
        "url": "https://github.com/scale-tone/kee-shepherd/issues"
    },
    "icon": "logo.png",
    "keywords": [
        "secrets",
        "credentials",
        "security",
        "azure",
        "key vault",
        "github",
        "codespaces"
    ],
    "publisher": "kee-shepherd",
    "license": "MIT",
    "activationEvents": [
        "*"
